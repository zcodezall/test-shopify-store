            "icon": "box",
            "row_content": "",
            "liquid": "<style>\n.product-specs {\n  display: flex;\n  border: 1px solid red;\n}\n\n.product-specs__column {\n  flex: 1;\n  border-right: 1px solid red;\n  padding: 0 10px;\n}\n\n.product-specs__column:last-child {\n  border-right: none;\n}\n<\/style>\n\n<div class=\"product-specs\">\n  <div class=\"product-specs__column\">\n    <h3>Features<\/h3>\n    {% if product.description %}\n     {{product.description}}\n    {% else %}\n      <p>No features available.<\/p>\n    {% endif %}\n  <\/div>\n\n  <div class=\"product-specs__column\">\n    <h3>Specifications<\/h3>\n <% if product.metafields.product_color %>Product Colour  \n{{ product.metafields.product_color.value}}\n\n<% endif %>\n \n\n\n\n\n  <\/div>\n\n  <div class=\"product-specs__column\">\n    <h3>Assembly<\/h3>\n    {% if product.assembly %}\n      {{ product.assembly }}\n    {% else %}\n      <p>No assembly information available.<\/p>\n    {% endif %}\n  <\/div>\n\n  <div class=\"product-specs__column\">\n    <h3>Warranty<\/h3>\n    {% if product.warranty %}\n      {{ product.warranty }}\n    {% else %}\n      <p>No warranty information available.<\/p>\n    {% endif %}\n  <\/div>\n<\/div>",
