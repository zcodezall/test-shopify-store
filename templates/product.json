        "25ba80e4-564d-4d1a-ad0c-0eaef876103a": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<script>\n  document.addEventListener(\"DOMContentLoaded\", function() {\n    setTimeout(function() {\n      var accordionContent = document.querySelector(\".accordion__content\");\n      if (accordionContent && accordionContent.innerHTML.trim() === \"\") {\n        var collapsibleField = accordionContent.closest(\".product__accordion\");\n        if (collapsibleField) {\n          collapsibleField.style.display = \"none\";\n        }\n      } else {\n        var pseudoElement = window.getComputedStyle(accordionContent, \"::after\");\n        if (pseudoElement && pseudoElement.content === \"\") {\n          var collapsibleField = accordionContent.closest(\".product__accordion\");\n          if (collapsibleField) {\n            collapsibleField.style.display = \"none\";\n          }\n        }\n      }\n    }, 3000);\n  });\n<\/script>\n\n{% assign hasSwatch = false %}\n{% assign firstSwatchVariant = null %}\n{% assign swatchCode = \"\" %}\n\n{% if product.variants.size > 1 %}\n  {% for variant in product.variants %}\n    {% if variant.metafields.custom.has_swatch == 'true' and firstSwatchVariant == null %}\n      {% assign hasSwatch = true %}\n      {% assign firstSwatchVariant = variant %}\n      {% assign swatchCode = variant.metafields.custom.swatchitem %}\n    {% endif %}\n  {% endfor %}\n{% else %}\n  {% if product.metafields.custom.has_swatch == 'true' %}\n    {% assign hasSwatch = true %}\n    {% assign swatchCode = product.metafields.custom.swatchitem %}\n  {% endif %}\n{% endif %}\n\n{% if hasSwatch and firstSwatchVariant != null %}\n<div class=\"abc\">\n  <a class='swatchLink' href=\"#\" onclick=\"addSwatchToCart('{{ firstSwatchVariant.product.title }}', '{{ swatchCode }}')\">Request a Swatch<\/a>\n<\/div>\n{% endif %}\n\n{% if hasSwatch %}\n<script>\n  function addSwatchToCart(productTitle, swatchCode) {\n    event.preventDefault();\n    var formData = new FormData();\n    formData.append('id', 45278378852636); \/\/ Replace 'YOUR_PRODUCT_ID' with the ID of your dummy swatch product\n    formData.append('properties[For]', productTitle);\n    formData.append('properties[Swatch Code]', swatchCode);\n\n    fetch('\/cart\/add.js', {\n      method: 'POST',\n      body: formData\n    }).then(function(response) {\n      return response.json();\n    }).then(function(data) {\n      \/\/ Handle the response data or perform any necessary actions after adding the swatch product to the cart\n      console.log(data);\n      window.location.href = '\/cart';\n    }).catch(function(error) {\n      \/\/ Handle any errors that occur during the request\n      console.error(error);\n    });\n  }\n<\/script>\n\n\n\n{% endif %}"
          }
        },
